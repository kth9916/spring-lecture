<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.springbook.biz.board.BoardVO" alias="board"></typeAlias>
		<typeAlias type="com.springbook.biz.user.UserVO" alias="user"></typeAlias>
		<typeAlias type="com.mybatis.join.vo.DepartmentVO" alias="dept"/>
		<typeAlias type="com.mybatis.join.vo.EmployeeVO" alias="emp"/>
		<typeAlias type="com.mybatis.join.vo.JoinVO" alias="joinvo"/>
		<typeAlias type="com.mybatis.join.vo.SpringDepartmentVO" alias="sdept"/>
		<typeAlias type="com.mybatis.join.vo.SpringEmploymentVO" alias="semp"/>
		<typeAlias type="com.mybatis.join.vo.SpringJoinVO" alias="sjoinvo"/>
		<typeAlias type="com.mybatis.join.vo.End_VO" alias="end_vo"/>
		<typeAlias type="com.mybatis.join.vo.AllJoinVO" alias="alljoinvo"/>
		<typeAlias type="com.mybatis.join.vo.JoinVO2" alias="joinvo2"/>
		<typeAlias type="com.mybatis.join.vo.EmpsVO" alias="emps"/>
		<typeAlias type="com.mybatis.join.vo.DeptsVO" alias="depts"/>
		<typeAlias type="com.mybatis.join.vo.LocationVO" alias="location"/>
		<typeAlias type="com.mybatis.join.vo.JobVO" alias="job"/>
	</typeAliases>
	
	 
	<mappers>
		<mapper resource="mappings/board-mapping.xml" />
		<mapper resource="mappings/user-mapping.xml" />
		<mapper resource="mappings/join-mapping.xml"/>
		<mapper resource="mappings/sjoin-mapping.xml"/>
	</mappers>
</configuration>
